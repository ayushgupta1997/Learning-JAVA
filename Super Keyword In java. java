/**
 *
 * @author Ayush Gupta
 */
public class Telusko {
    public static void main(String[] args) {
         B obj = new B();
         obj.show();
         
    }  
}
class A {
    public void show() {
        System.out.println("In A");
    }
} 
class B extends A {
    
    public void show() {
        super.show();                    // super keyword will call the parent class function even when we created object of Base class
        System.out.println("In B");
    }
  
}
